{
    "headers": [
        {
            "source": "**/*.@(jpg|jpeg|gif|png|webp|svg|ico|css|js)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "**/*.@(html|json)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache"
                }
            ]
        }
    ]
}